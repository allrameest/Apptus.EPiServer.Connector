    <panel name="/search-provider-panels/key-lookup" public="true" read_only="true">
        <definition>
            <reference ordinal="1" role="/">
                <name>/esales/navigate/product-list</name>
                <arguments>
                    <argument>
                        <name>facets</name>
                        <value></value>
                    </argument>
                    <argument>
                        <name>filter</name>
                        <value>_id:'${id}' AND locale_filter:'${locale_filter}'</value>
                    </argument>
                    <argument>
                        <name>locale</name>
                        <value></value>
                    </argument>
                    <argument>
                        <name>search_attributes</name>
                        <value></value>
                    </argument>
                    <argument>
                        <name>search_phrase</name>
                        <value></value>
                    </argument>
                    <argument>
